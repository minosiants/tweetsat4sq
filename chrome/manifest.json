{
  "name": "tweets@foursquare",
  "version": "1.0.2",
  "icons" : { "48": "4sq-logo48.png",
             "128": "4sq-logo128.png" },
  "description":
      "Shows tweets at foursquare venues location",
  "homepage_url": "http://minosiants.com",
  "permissions": [        
    "idle",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "background_page": "background.html",
  "content_scripts": [{
    "matches": ["https://foursquare.com/v/*", "http://foursquare.com/v/*"],
    "css": ["jquery.jtweetsanywhere-1.3.1.css","content.css"],
    "js": ["jquery-1.7.1.min.js","jquery.jtweetsanywhere-1.3.1.js","content.js"],
    "all_frames": true
  }]
}
