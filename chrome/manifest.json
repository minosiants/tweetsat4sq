{
  "name": "foursquare and tweets",
  "version": "1.0.3",
  "description":
      "Shows tweets of foursquare venues location",
  "homepage_url": "http://minosiants.com",
  "permissions": [    
    "history",
    "idle",
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background_page": "background.html",
  "content_scripts": [{
    "matches": ["https://foursquare.com/v/*", "http://foursquare.com/v/*"],
    "css": ["jquery.jtweetsanywhere-1.3.1.css","content.css"],
    "js": ["jquery-1.7.1.min.js","jquery.jtweetsanywhere-1.3.1.js","content.js"],
    "all_frames": true
  }]
}
